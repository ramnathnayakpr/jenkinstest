  agent any
  stages {  
    stage('Build') {
      steps {
        sh 'echo Building'
      }
      steps {
        sleep 5
      }
    }  
    stage('Test') {
      steps {
        sh 'echo Building'
      }
      steps {
        sleep 5
      }
    }  
    stage('Deploy') {
      steps {
        sh 'echo Deploying'
      }
      steps {
        sleep 5
      }
    }    
  }
}
